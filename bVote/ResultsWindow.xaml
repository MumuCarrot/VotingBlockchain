<Window x:Class="bVote.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bVote"
        mc:Ignorable="d"
        Title="ResultsWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="75"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid x:Name="GridForChart" Grid.Column="1" Background="LightGray"  />
        <Button Content="Save" Grid.Column="0" Name="SaveBtn" Margin="10,100,10,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
