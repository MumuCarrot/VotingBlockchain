<Window x:Class="bVote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bVote"
        mc:Ignorable="d"
        Title="bVote" Height="800" Width="600">
    <Window.Background>
        <LinearGradientBrush EndPoint="0,1" StartPoint="1,0">
            <GradientStop Color="#AFFD86"/>
            <GradientStop Color="#BAC3F3" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>

    <Frame NavigationUIVisibility="Hidden" Content="{Binding Navigation.CurrentPage}"/>
</Window>
